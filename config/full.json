{
  "name": "full",
  "width": 480,
  "height": 320,
  "default_font_size": 16,
  "font_families": [
    "DejaVu Sans Mono",
    "Liberation Mono",
    "Consolas",
    "Monaco",
    "Menlo",
    "Ubuntu Mono",
    "Courier New",
    "monospace"
