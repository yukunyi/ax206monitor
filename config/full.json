{
  "name": "full",
  "width": 480,
  "height": 320,
  "font_sizes": {
    "min_label": 8,
    "def_label": 10,
    "default": 12,
    "small": 8,
    "large": 14
  },
  "font_families": [
    "DejaVu Sans Mono",
    "Liberation Mono",
    "Consolas",
    "Monaco",
    "Menlo",
    "Ubuntu Mono",
    "Courier New",
    "monospace"
  ],
  "output_type": "ax206usb",
  "output_file": "",
  "refresh_interval": 1000,
  "history_size": 200,
  "colors": {
    "default_text": "#ffffff",
    "default_background": "#2a2a2a",
    "header_text": "#74b9ff",
    "header_background": "#1a1a1a",
    "progress_background": "#3a3a3a",
    "value_normal": "#4ecdc4",
    "progress_fill": "#4ecdc4",
    "chart_line": "#45b7d1"
  },
  "items": [
    {
      "type": "value", "monitor": "current_time", "x": 5, "y": 5,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "load_avg", "x": 162, "y": 5,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "disk_temp", "x": 319, "y": 5,
      "width": 156, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },

    {
      "type": "value", "monitor": "cpu_usage", "x": 5, "y": 63,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "cpu_temp", "x": 162, "y": 63,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "cpu_freq", "x": 319, "y": 63,
      "width": 156, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },

    {
      "type": "value", "monitor": "memory_usage", "x": 5, "y": 121,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "memory_used", "x": 162, "y": 121,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "memory_total", "x": 319, "y": 121,
      "width": 156, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },

    {
      "type": "value", "monitor": "gpu_usage", "x": 5, "y": 179,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "gpu_temp", "x": 162, "y": 179,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "gpu_freq", "x": 319, "y": 179,
      "width": 156, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },

    {
      "type": "value", "monitor": "net_default_ip", "x": 5, "y": 237,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "net_default_upload", "x": 162, "y": 237,
      "width": 155, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },
    {
      "type": "value", "monitor": "net_default_download", "x": 319, "y": 237,
      "width": 156, "height": 56, "font_size": 12, "bg": "#2a2a2a"
    },

    {
      "type": "text",
      "text": "Repository: https://github.com/yukunyi/ax206monitor",
      "x": 5,
      "y": 295,
      "width": 470,
      "height": 20,
      "font_size": 10,
      "bg": "#333333"
    }
  ]
