{
  "name": "normal",
  "width": 480,
  "height": 320,
  "font_sizes": {
    "min_label": 14,
    "def_label": 16,
    "default": 14,
    "small": 12,
    "large": 18
  },
  "font_families": [
    "DejaVu Sans Mono",
    "Liberation Mono",
    "Consolas",
    "Monaco",
    "Menlo",
    "Ubuntu Mono",
    "Courier New",
    "monospace"
  ],
  "output_type": "ax206usb",
  "output_file": "monitor_normal.png",
  "refresh_interval": 1000,
  "history_size": 120,
  "colors": {
    "default": "#ffffff",
    "cpu_temp": "#ff6b6b",
    "cpu_usage": "#4ecdc4",
    "cpu_freq": "#45b7d1",
    "memory_usage": "#f7b731",
    "memory_used": "#a55eea",
    "gpu_temp": "#fd79a8",
    "gpu_usage": "#fdcb6e",
    "network_upload": "#00b894",
    "network_download": "#00cec9",
    "network_ip": "#74b9ff",
    "current_time": "#ddd"
  },
  "items": [
    {
      "type": "value",
      "monitor": "net_default_ip",
      "x": 5,
      "y": 5,
      "width": 150,
      "height": 25,
      "font_size": 14,
      "bg": "#1a1a1a",
      "label": false
    },
    {
      "type": "value",
      "monitor": "net_default_name",
